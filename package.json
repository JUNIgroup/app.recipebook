{
  "name": "de.junigroup.app.recipebook",
  "private": true,
  "version": "0.1.0",
  "description": "Private collection of recipes.",
  "author": "JUNIgroup",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "vitest",
    "eslint",
    "prettier"
  ],
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build:only": "vite build",
    "preview": "vite preview",
    "test": "vitest --run --coverage",
    "test:watch": "vitest",
    "test:watch:verbose": "vitest --coverage",
    "test:ui": "vitest --ui",
    "emulator": "firebase emulators:start --only auth",
    "lint": "npm run eslint && npm run prettier && npm run stylelint",
    "eslint": "eslint . --cache",
    "eslint:fix": "eslint . --cache --fix --ext .ts,.tsx",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "stylelint": "stylelint ./src/**/*.scss ./src/**/*.css",
    "stylelint:fix": "stylelint --fix ./src/**/*.scss ./src/**/*.css",
    "clean": "shx rm -rf coverage dist .eslintcache",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JUNIgroup/app.recipebook.git"
  },
  "bugs": {
    "url": "https://github.com/JUNIgroup/app.recipebook/issues"
  },
  "homepage": "https://github.com/JUNIgroup/app.recipebook#readme",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.2",
    "axios": "^1.3.4",
    "firebase": "^9.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "rxjs": "^7.8.0",
    "superstruct": "^1.0.3",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.28.3",
    "@vitest/ui": "^0.28.3",
    "c8": "^7.12.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fake-indexeddb": "^4.0.1",
    "happy-dom": "^8.2.0",
    "htmlhint": "^1.1.4",
    "husky": "^8.0.3",
    "jest-extended": "^3.2.4",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "short-unique-id": "^4.4.4",
    "shx": "^0.3.4",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vitest": "^0.28.3"
  }
}
