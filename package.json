{
  "name": "de.junigroup.app.recipebook",
  "private": true,
  "version": "0.1.0",
  "description": "Private collection of recipes.",
  "author": "JUNIgroup",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "vitest",
    "eslint",
    "prettier"
  ],
  "type": "module",
  "scripts": {
    "start": "vite --host",
    "build": "tsc && vite build",
    "build:only": "vite build",
    "preview": "vite preview",
    "test": "vitest --run --coverage",
    "test:watch": "vitest",
    "test:watch:verbose": "vitest --coverage",
    "test:ui": "vitest --ui --no-open",
    "test:ui:open": "vitest --ui",
    "emulator": "kill-port --port 4000,4001,9099,9098,4500,9150 && firebase emulators:start --only auth,firestore",
    "analyze:deadcode": "npx deadfile ./src/main.tsx --dir ./src --exclude main.tsx \\.spec\\. \\.test-helper\\. ^mock- \\.samples\\.ts \\.d\\.ts",
    "analyze:bundlesize": "vite build --sourcemap && npx source-map-explorer dist/assets/*.js",
    "lint": "npm run eslint && npm run prettier && npm run stylelint",
    "eslint": "eslint . --cache",
    "eslint:fix": "eslint . --cache --fix --ext .ts,.tsx",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "stylelint": "stylelint \"./src/**/*.{scss,css}\"",
    "stylelint:fix": "stylelint --fix ./src/**/*.scss ./src/**/*.css",
    "clean": "shx rm -rf coverage dist .eslintcache",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JUNIgroup/app.recipebook.git"
  },
  "bugs": {
    "url": "https://github.com/JUNIgroup/app.recipebook/issues"
  },
  "homepage": "https://github.com/JUNIgroup/app.recipebook#readme",
  "dependencies": {
    "@fontsource-variable/open-sans": "^5.0.6",
    "@reduxjs/toolkit": "^1.9.3",
    "firebase": "^9.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "rxjs": "^7.8.0",
    "superstruct": "^1.0.3",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^3.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.7",
    "@vitest/ui": "^0.29.7",
    "c8": "^7.13.0",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fake-indexeddb": "^4.0.1",
    "happy-dom": "^8.9.0",
    "htmlhint": "^1.1.4",
    "husky": "^8.0.3",
    "jest-extended": "^3.2.4",
    "kill-port": "^2.0.1",
    "lint-staged": "^13.2.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "short-unique-id": "^4.4.4",
    "shx": "^0.3.4",
    "stylelint": "^15.3.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vitest": "^0.29.7"
  }
}
